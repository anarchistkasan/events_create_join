{
  "parser": "babel-eslint",
  //"extends": ["airbnb"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "no-console": 0,
    "react/destructuring-assignment": ["never"],
    "react/prefer-stateless-function": 1
  },
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.config.dev.js"
      }
    }
  },
  "globals": {
    "config": true
  }
}
